	rm -fr $$distdir && \
	tar xfz $$distdir.tar.gz && \
	mkdir $$distdir/debian && \
	cp pkg/debian-common/* $$distdir/debian && \
	cp pkg/$*/* $$distdir/debian && \
	cd $$distdir && \
	debuild -S -sa

